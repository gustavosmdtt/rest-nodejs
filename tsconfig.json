{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": "./"
  },
    "include": [
    "tests/cypress/e2e/**/*.ts",
    "tests/cypress/api/**/*.ts",
    "tests/cypress/support/**/*.ts",
    "tests/cypress/types/**/*.ts"
  ]
}
